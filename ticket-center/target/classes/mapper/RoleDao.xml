<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ticketcenter.dao.RoleDao">


    <select id="getRoleList" resultType="com.ticketcenter.model.SystemRole">
        select id,sys_role_type as sysRoleType,sys_role_desc as sysRoleDesc from t_sys_role
        order by id
    </select>

</mapper>